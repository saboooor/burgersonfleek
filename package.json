{
  "name": "burgersonfleek",
  "description": "Website for Burgers on Fleek",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages dev ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint src/**/*.ts*",
    "postinstall": "prisma generate --accelerate",
    "preview": "qwik build preview && vite preview --open",
    "prisma:generate": "prisma generate --accelerate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "serve": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "start": "vite --open --mode ssr --host",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.9",
    "@builder.io/qwik-city": "1.2.9",
    "@prisma/client": "5.2.0",
    "@prisma/extension-accelerate": "^0.6.0",
    "@types/eslint": "8.44.2",
    "@types/node": "latest",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "autoprefixer": "10.4.15",
    "eslint": "8.45.0",
    "eslint-plugin-qwik": "1.2.9",
    "node-fetch": "3.3.2",
    "postcss": "8.4.28",
    "prisma": "^5.2.0",
    "qwik-ionicons": "1.0.5",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "undici": "5.23.0",
    "uuid": "^9.0.0",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.0",
    "wrangler": "latest",
    "zod": "^3.22.2"
  }
}
